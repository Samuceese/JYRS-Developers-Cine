deleteAllClientes:
DELETE FROM UsuariosEntity WHERE tipo != 'admin';

selectAll:
SELECT * FROM UsuariosEntity ORDER BY nombre ASC;

selectByEmail:
SELECT * FROM UsuariosEntity WHERE email = ?;

selectById:
SELECT * FROM UsuariosEntity WHERE id = ?;

selectLastInserted:
SELECT * FROM UsuariosEntity WHERE id = last_inserted_rowid();

insert:
INSERT INTO UsuariosEntity (id, email, nombre, apellidos, tipo, contrasena)
VALUES (?, ?, ?, ?, ?, ?);

update:
UPDATE UsuariosEntity SET contrasena = ?;

