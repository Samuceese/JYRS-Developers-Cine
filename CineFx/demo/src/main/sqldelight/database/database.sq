CREATE TABLE IF NOT EXISTS UsuariosEntity(
    id TEXT PRIMARY KEY NOT NULL,
    email TEXT NOT NULL,
    nombre TEXT NOT NULL,
    apellidos TEXT NOT NULL,
    tipo TEXT NOT NULL,
    contrasena TEXT NOT NULL
);

